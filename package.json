{
  "name": "zengo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate": "turbo run generate --filter dapp",
    "add-component": "turbo run add-component --filter dapp",
    "contracts:node": "turbo run run-node --filter contracts",
    "contracts:build": "turbo run build --filter contracts",
    "contracts:deploy": "turbo run deploy --filter contracts"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": ">=18.16.0"
  },
  "pnpm": {
    "overrides": {
      "lodash@<4.17.11": ">=4.17.11",
      "lodash@<4.17.5": ">=4.17.5",
      "flat@<5.0.1": ">=5.0.1",
      "minimatch@<3.0.2": ">=3.0.2",
      "lodash@<4.17.20": ">=4.17.20",
      "lodash@<4.17.21": ">=4.17.21",
      "lodash@<4.17.12": ">=4.17.12",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "minimatch@<3.0.5": ">=3.0.5"
    }
  }
}
